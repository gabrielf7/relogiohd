{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "author": "Gabrielf7",
  "version": "2.2.1",
  "default_locale": "pt",
  "icons": {
    "16": "./src/relogio-icon-16x16.png",
    "32": "./src/relogio-icon-32x32.png",
    "48": "./src/relogio-icon-48x48.png",
    "128": "./src/relogio-icon-128x128.png"
  },
  "browser_action": {
    "default_title": "Rel√≥gio HD",
    "default_popup": "./extension/firefox/popup/popup.html"
  },
  "background": {
    "page": "./extension/firefox/background/background.html",
    "persistent": false
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "./extension/firefox/options/config.html"
  },
  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "management"
  ],
  "homepage_url": "https://github.com/gabrielf7/relogiohd"
}